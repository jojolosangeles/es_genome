set -x
rm -rf data
curl -XDELETE localhost:9200/test_index | jq .
sleep 3
mkdir -p data
python gen.py --size 1000 --alpha ACGT --n 10 > data/ACGT.txt
python gen_analyzer.py --size 3 --alpha ACGT --n 2 --analyzer analyzer_1 > data/analyzer_1.json
curl -XPUT 'localhost:9200/test_index?pretty' -d @data/mapping_1.json -H 'Content-Type: application/json'
curl -XPUT 'localhost:9200/test_index?pretty' -d @data/analyzer_1.json -H 'Content-Type: application/json'
sleep 3
python ../es_loader/line_loader.py test_index ./data/ACGT.txt sp1 ch1
sleep 3
curl localhost:9200/test_index/_search | jq .
curl localhost:9200/test_index/_count | jq .

