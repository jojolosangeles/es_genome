set -x
curl -XDELETE 'localhost:9200/filter2'
curl -XPUT 'localhost:9200/filter2?pretty' -H 'Content-Type: application/json' -d @filter2.json
curl -XPOST 'localhost:9200/filter2/_analyze?pretty' -H 'Content-Type: application/json' -d'
{
    "analyzer": "my_analyzer",
    "text": "abcdefghijklmnopqrstuvwxyz"
}'
curl -XPOST 'localhost:9200/filter2/_analyze?pretty' -H 'Content-Type: application/json' -d'
{
    "analyzer": "my_analyzer",
    "text": "abajkdjfkasjdfas;dlkjf;alskdjfabcdefghasdfjaskdfjasdabcabcabdjdksdjfskdjfsjdkflsdjfksdjkfjskdlhtkhsdhkshkhdsjfksdjfkjasdkg;asldkjf;;laksjd fl;kajsdffjkdsbhabsjjbsadbasdbasdbbajsbajcdefghijklmnopqrstuvwxyz"
}'